{
  "settings": {
    "max_result_window": 10000,
    "index.mapping.ignore_malformed": false,
    "index": {
      "number_of_shards": 1,
      "number_of_replicas": 1,
      "analysis": {
        "analyzer": {
          "autocomplete_analyzer": {
            "type": "custom",
            "tokenizer": "lowercase",
            "filter": [
              "asciifolding",
              "title_ngram"
            ]
          },
          "autocomplete_analyzer_for_code": {
            "type": "custom",
            "tokenizer": "whitespace",
            "filter": [
              "lowercase",
              "asciifolding",
              "title_ngram"
            ]
          }
        },
        "filter": {
          "title_ngram": {
            "type": "nGram",
            "min_gram": 3,
            "max_gram": 5
          }
        }
      }
    }
  },
  "mappings": {
    "doc": {
      "properties": {
        "path": {
          "type": "keyword"
        },
        "startDate": {
          "type": "date"
        },
        "longitude": {
          "type": "float"
        },
        "latitude": {
          "type": "float"
        },
        "authCode": {
          "type": "text",
          "analyzer": "autocomplete_analyzer_for_code",
          "fields": {
            "raw": {
              "type": "keyword"
            }
          }
        },
        "userCode": {
          "type": "text",
          "analyzer": "autocomplete_analyzer_for_code",
          "fields": {
            "raw": {
              "type": "keyword"
            }
          }
        },
        "name": {
          "type": "text",
          "analyzer": "autocomplete_analyzer",
          "fields": {
            "raw": {
              "type": "keyword"
            }
          }
        }
      }
    }
  }
}

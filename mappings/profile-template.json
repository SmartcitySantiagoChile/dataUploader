{
  "settings": {
    "max_result_window": 15000,
    "index": {
      "number_of_shards": 16,
      "number_of_replicas": 1,
      "analysis": {
        "analyzer": {
          "autocomplete_analyzer": {
            "type": "custom",
            "tokenizer": "lowercase",
            "filter": [
              "asciifolding",
              "title_ngram"
            ]
          },
          "autocomplete_analyzer_for_code": {
            "type": "custom",
            "tokenizer": "whitespace",
            "filter": [
              "lowercase",
              "asciifolding",
              "title_ngram"
            ]
          }
        },
        "filter": {
          "title_ngram": {
            "type": "nGram",
            "min_gram": 3,
            "max_gram": 5
          }
        }
      }
    }
  },
  "mappings": {
    "doc": {
      "properties": {
        "path": {
          "type": "keyword"
        },
        "operator": {
          "type": "byte"
        },
        "route": {
          "type": "keyword"
        },
        "userRoute": {
          "type": "keyword"
        },
        "licensePlate": {
          "type": "keyword",
          "index": false
        },
        "authStopCode": {
          "type": "text",
          "analyzer": "autocomplete_analyzer_for_code",
          "fields": {
            "raw": {
              "type": "keyword"
            }
          }
        },
        "userStopCode": {
          "type": "text",
          "analyzer": "autocomplete_analyzer_for_code",
          "fields": {
            "raw": {
              "type": "keyword"
            }
          }
        },
        "userStopName": {
          "type": "text",
          "analyzer": "autocomplete_analyzer",
          "fields": {
            "raw": {
              "type": "keyword"
            }
          }
        },
        "expeditionStartTime": {
          "type": "date",
          "format": "yyyy-MM-dd HH:mm:ss"
        },
        "expeditionEndTime": {
          "type": "date",
          "format": "yyyy-MM-dd HH:mm:ss"
        },
        "fulfillment": {
          "type": "keyword"
        },
        "expeditionStopOrder": {
          "type": "short",
          "index": false
        },
        "expeditionDayId": {
          "type": "integer",
          "index": false
        },
        "stopDistanceFromPathStart": {
          "type": "half_float",
          "index": false
        },
        "expandedBoarding": {
          "type": "half_float",
          "index": false
        },
        "expandedAlighting": {
          "type": "half_float",
          "index": false
        },
        "loadProfile": {
          "type": "half_float",
          "index": false
        },
        "busCapacity": {
          "type": "short",
          "index": false
        },
        "expeditionStopTime": {
          "type": "text",
          "index": false
        },
        "timePeriodInStartTime": {
          "type": "byte"
        },
        "timePeriodInStopTime": {
          "type": "byte"
        },
        "dayType": {
          "type": "byte"
        },
        "busStation": {
          "type": "byte"
        },
        "transactions": {
          "type": "integer",
          "index": false
        },
        "halfHourInStartTime": {
          "type": "byte"
        },
        "halfHourInStopTime": {
          "type": "byte"
        },
        "notValid": {
          "type": "byte"
        }
      }
    }
  }
}
